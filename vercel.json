{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    {
      "src": "/api/webhooks/user",
      "methods": ["POST"],
      "headers": {
        "x-vercel-protection-bypass": "1"
      },
      "dest": "/api/webhooks/user"
    }
  ],
  "functions": {
    "src/app/api/webhooks/**/*.ts": {
      "maxDuration": 10
    }
  }
}